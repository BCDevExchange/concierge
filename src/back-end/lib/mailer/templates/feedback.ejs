<!doctype html>
<html style="">
  <head>
    <meta charset="utf8" />
  </head>
  <body style="font-family: sans-serif; text-align: center; padding: 32px 16px;">
    <table style="width: 100%; max-width: 400px; min-width: 300px; margin: 0 auto 32px auto;">
      <tr>
        <td class="logo" style="padding: 16px; background: #003366; border-radius: 4px; text-align: center;">
          <a href="<%= data.logo.href %>" target="_blank" style="margin: 0 auto; text-align: center;">
            <img src="<%= data.logo.src %>" alt="<%= data.logo.alt %>" style="height: 36px; margin: 0 auto;"/>
          </a>
        </td>
      </tr>
      <tr>
        <td class="title" style="padding-top: 48px;">
          <h1 style="margin: 0 auto; text-align: center; font-size: 22px; line-height: 1.2; padding: 0 16px;">
            Feedback Received
          </h2>
        </td>
      </tr>
      <% if (data.feedback.userType) { %>
      <tr>
        <td class="user-type" style="padding: 0 16px; padding-top: 32px; margin: 0 auto; text-align: left; font-size: 16px; line-height: 1.3;">
          <b>User Type:&nbsp;</b>
          <%= data.feedback.userType %>
        </td>
      </tr>
      <% } %>
      <tr>
        <td class="rating" style="padding: 0 16px; padding-top: 32px; margin: 0 auto; text-align: left; font-size: 16px; line-height: 1.3;">
          <b>Rating:&nbsp;</b>
          <%= data.feedback.rating%>
        </td>
      </tr>
      <tr>
        <td class="text" colspan="2" style="padding: 0 16px; padding-top: 32px; margin: 0 auto; text-align: left; font-size: 16px; line-height: 1.3; white-space: pre-line;">
          <%= data.feedback.text %>
        </td>
      </tr>
    </table>
  </body>
</html>
